using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Finding_Max
{
    class Max
    {
        public int Great(int num1, int num2)
        {
            if (num1 > num2)
                return num1;
            else
                return num2;

        }
       public static int Read()
        {
            return int.Parse(Console.ReadLine());
        }
        static void Main(string[] args)
        {
            int i = 0;
            for (i = 0; ; i++)
            {
                Max c = new Max();
                int a = Read();
                int b = Read();
                int result = c.Great(a, b);
                Console.WriteLine(result);
                
            }

        }
    }
}
